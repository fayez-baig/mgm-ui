{
	"name": "mgm-ui-lib",
	"version": "0.0.1",
	"description": "React components",
	"keywords": [
		"react",
		"css",
		"ui",
		"component",
		"components",
		"react-component",
		"library"
	],
	"homepage": "https://mgm-ui.web.app/",
	"bugs": {
		"url": "https://github.com/fayez-baig/mgm-ui.git/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fayez-baig/mgm-ui.git"
	},
	"license": "MIT",
	"author": {
		"name": "Faran, Fayez, Rifaq, Sobiya",
