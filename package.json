		"eslint-plugin-typescript-sort-keys": "^1.3.0",
		"eslint-plugin-unused-imports": "^0.1.3",
		"material-icons": "^0.3.1",
		"prettier": "^2.1.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-is": "^16.13.1",
		"react-scripts": "^3.4.3",
		"react-test-renderer": "^16.13.1",
		"rimraf": "^3.0.2",
		"semantic-release": "^17.1.1",
